@model Corrida

<div class="card border-primary" style="max-width: 100%;">
    <div class="card-header">
        <h5 class="card-text">Acompanhar Corrida</h5>
    </div>
    <div class="card-body">
        <div class="card">
            <div class="row">
                <label>Origem:</label>
                <div class="col">
                    <text>@Model.Origem</text>
                </div>
            </div>
            <button type="submit">Solicitar</button>
        </div>
    </div>
</div>